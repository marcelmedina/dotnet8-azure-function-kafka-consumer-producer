{
  "version": "2.0",
  "extensions": {
    "kafka": {
      "maxBatchSize": 3,
      "SubscriberIntervalInSeconds": 1,
      "ExecutorChannelCapacity": 1,
      "ChannelFullRetryIntervalInMs": 50
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}